{
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "env": {
        "webextensions": true,
        "browser": true,
        "es6": true
    }
}
